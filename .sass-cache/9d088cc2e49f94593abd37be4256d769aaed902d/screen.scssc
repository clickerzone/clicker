// =============== Equipo =================
.va-container{
  position:relative;
  margin:40px auto 0 auto;

}
.va-wrapper{
  width:100%;
  height:100%;
  position:relative;
  overflow:hidden;
  // background:#000;
  margin-top:8em;
  // box-shadow: 0 0 5px rgba(0,0,0,0.8);
}
.va-slice{
  position:absolute;
  width:100%;
  left:0px;
  overflow:hidden;
  border-top: 3px solid rgba(94,193,149,0.5);
}
.va-slice-1{
  background:url(../img/content/diana.jpg) no-repeat center center;
}
.va-slice-2{
  background:url(../img/content/josue.jpg) no-repeat center center;
}
.va-slice-3{
  background:url(../img/content/guille.jpg) no-repeat center center;
}
.va-slice-4{
  background:url(../img/content/edison.jpg) no-repeat center center;
}
.va-slice-5{
  background:url(../img/content/luis.jpg) no-repeat center center;
}

.va-title{
  font-family:$light;
  text-transform:uppercase;
  font-size:4em;
  width:1200px;
  margin:0 auto;
  color:#fff;
  text-shadow: 0px 0px 1px white;
}
.va-content{
  display:none;
  width:1200px;
}
.va-slice p{
  color:rgba(94,193,149,0.8);
  font-size: 2em;
  font-family:$regular;
  margin-left:3%;
}
.va-slice ul{
  margin-top:20px;
  font-family:$book;
  width:1200px;
  margin-left:3%;
}
.va-slice ul li{
  float:left;
  margin:0px 2px;
  clear:both;
  margin-bottom:1.2em;
  &:nth-of-type(1){
    color:white;
    width:500px;
    font-size:1.3em;
    letter-spacing:0.1em;
    text-shadow:1px 1px rgba(0,0,0,0.2);
  }
}
.va-slice ul li a{
  background:rgba(94,193,149,0.8);
  color:white;
  padding:3px 6px;
  font-size:2em;
  font-family:$book;
  text-transform:uppercase;
  text-decoration:none;
  display:block;
  @include transition(all,0.5s,ease-in);
}
.va-slice ul li a:hover{
  background:#fff;
  text-shadow:none;
  color:rgba(252,196,97,1);
}
.va-nav span{
  width:40px;
  height:25px;
  background:transparent url(../img/layout/prev.png) no-repeat center center;
  position:absolute;
  top:-35px;
  left:50%;
  margin-left:-20px;
  text-indent:-9000px;
  opacity:0.7;
  cursor:pointer;
  display:none;
  z-index:100;
}
.va-nav span.va-nav-next{
  background-image:url(../img/layout/next.png);
  top:auto;
  bottom:-35px;
}
.va-nav span:hover{
  opacity:1.0;
}


#video_background {
  position: fixed; 
  left: 0px; 
  top: 0px; 
  width: 100%; 
  height: 100%; 
  width: auto; 
  height: auto; 
  z-index: -1000; 
  overflow: hidden; 
  -webkit-filter: blur(3px);

}
#video_pattern { 
  background:url(../img/layout/somosBg.png);
  position: fixed;  
  left: 0px; 
  top: 0px; 
  width: 100%; 
  height: 100%; 
  z-index: -500; 
  opacity:0.7;
}


#contacto{
  position: relative;
  height:1000px;
  .infoContacto{
    width:1200px;
    margin:8em auto;
    z-index:200;
    form, > div{
      float:left;
      width:50%;
    }
     form{
           -webkit-box-sizing: border-box; /* Safari/Chrome, other WebKit */
   -moz-box-sizing: border-box;    /* Firefox, other Gecko */
   box-sizing: border-box;      
      padding-left:5%;
      label{
        display:block;
        color:white;
        font-family:$light;
        font-size:2em;
        margin-bottom:1em;
      }
      input{
        display:block;
        margin-top:0.5em; 
        height:2em;
        line-height:2em;
        width:60%;
        font-size:0.5em;
        padding-left:2%; 
         @include border-radius(4px);
      }
      textarea{
        font-size:0.5em;
        width:60%;
        padding-left:2%;
      }
      input[type="submit"]{
        font-size:1.2em;
        font-family:$regular;
        color:white;
        border:0;
        border-top:2px solid white;
        padding-top:0.5em;
        background:none;
        display:inline-block;
        letter-spacing:0.1em;
        &:hover{
          color:$aqua;
        }
      }
    }
    h3{
      font-size:3em;
      font-family:$regular;
      color:$aqua;
      letter-spacing:3px;
      margin-bottom:0.5em;
    }
    p{
      font-size: 2em;
      font-family:$thin;
      color:white;
      letter-spacing:3px;
      margin-bottom:0.5em;
      &:last-child{
        color:#82fef7;
      }
    }
    ul li{
      margin-top:2em;
      display:inline-block;
      width:8.5%;
      height:2.4em;
      a{
        height:2.4em;
        width:100%;
        &.fb{background-position: -85px 0;}
        &.tw{background-position: 0 0;}
        &.in{background-position: -156px 0;}
        &.sk{background-position: -235px 0;}
        &:hover{
          &.fb{background-position: -85px -67px;}
          &.tw{background-position: 0 -67px;}
          &.in{background-position: -156px -67px;}
          &.sk{background-position: -235px -67px;}
        }
      }
    }
  }
  .footer{
    position:relative;
    bottom:0;
    text-align:center;
    p{
      border-top:1px solid white; 
      padding-top:1em;
      width:50%;
      margin:0 auto;
      display:inline-block;
      color:white;
      font-size: 1em;
      font-family:$regular;
      letter-spacing:0.4em;
      @include transition(all,0.5s,ease-in);
      &:hover{
        color:$aqua;
      }
    }
  }
}


/* ===================================== Helper Clases ===================================== */

.sprite{
    background-image:url(../img/layout/sprites.png);
    background-repeat:no-repeat;
    display:block;
}



img{
	max-width:100%;
}

.ir {
    background-color: transparent;
    border: 0;
    overflow: hidden;
    *text-indent: -9999px;
}

.ir:before {
    content: "";
    display: block;
    width: 0;
    height: 150%;
}

.hidden {
    display: none !important;
    visibility: hidden;
}

.visuallyhidden {
    border: 0;
    clip: rect(0 0 0 0);
    height: 1px;
    margin: -1px;
    overflow: hidden;
    padding: 0;
    position: absolute;
    width: 1px;
}

.visuallyhidden.focusable:active,
.visuallyhidden.focusable:focus {
    clip: auto;
    height: auto;
    margin: 0;
    overflow: visible;
    position: static;
    width: auto;
}

.invisible {
    visibility: hidden;
}

.clearfix:before,
.clearfix:after {
    content: " ";
    display: table;
}

.clearfix:after {
    clear: both;
}

.clearfix {
    *zoom: 1;
}
