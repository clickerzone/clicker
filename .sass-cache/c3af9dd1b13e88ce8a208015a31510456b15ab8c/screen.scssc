/*=============== Equipo =================*/
#equipo{
  height:1100px;
  background:url(../img/layout/equipo.png);
  .infoEquipo{
    max-width:1200px;
    margin:0 auto;
    padding-top: 10em;
    z-index:200;
    h3{
      font-family: 'bebas_neuebook',sans-serif;
      color: #d2d1d1;
      font-size:4em;
      margin-bottom:0.7em;
      letter-spacing:0.1em;
    }
  }
}

.grid {
  margin: 40px auto 120px;
  max-width: 1250px;
  width: 90%;
  a {
    float: left;
    max-width: 250px;
    width: 25%;
    color: #333;
  }
  figure {
    position: relative;
    overflow: hidden;
    margin: 5px;
   /* background-color:rgba(45,233,255,0.2);*/
    img {
      position: relative;
      display: block;
      width: 100%;
      opacity: 0.8;
      -webkit-transition: opacity 0.3s;
      transition: opacity 0.3s;
    }
    figcaption {
      position: absolute;
      top: 0;
      z-index: 11;
      padding: 10px;
      width: 100%;
      height: 100%;
      text-align: center;
      h2 {
        height:2em;
        color: #3498db;
        text-transform: uppercase;
        letter-spacing: 1px;
        font-family: 'bebas_neuebook',sans-serif;
        font-weight: 600;
        font-size: 1.7em;
        -webkit-transition: -webkit-transform 0.3s;
        transition: transform 0.3s;
      }
      p {
        padding: 0 20px;
        color: #aaa;
        font-family: 'bebas_neuebook',sans-serif;
        font-size: 1.3em;
        font-weight: 300;
        -webkit-transition: opacity 0.3s, -webkit-transform 0.3s;
        transition: opacity 0.3s, transform 0.3s;
      }
    }
  }
}
 

.grid a:nth-child(odd) {
  margin: 30px 0 -30px 0;
}
.grid figcaption h2,
.grid figcaption p {
  -webkit-transform: translateY(50px);
  transform: translateY(50px);
}
.grid figure span {
  position: absolute;
  padding: 4px 20px;
  border: none;
  text-transform: uppercase;
  letter-spacing: 1px;
  font-weight: bold;
  -webkit-transition: opacity 0.3s, -webkit-transform 0.3s;
  transition: opacity 0.3s, transform 0.3s;
}
.grid figcaption,
.grid figcaption h2,
.grid figcaption p,
.grid figure span {
  -webkit-backface-visibility: hidden;
  backface-visibility: hidden;
}

/* Style for SVG */
.grid svg {
  position: absolute;
  top: -1px; /* fixes rendering issue in FF */
  z-index: 10;
  width: 100%;
  height: 100%;
}

.grid svg path {
  fill: #ecf0f1;
}

/* Hover effects */
.grid a:hover figure img {
  opacity: 1;
}

.grid a:hover figcaption h2,
.grid a:hover figcaption p {
  -webkit-transform: translateY(0);
  transform: translateY(0);
}

.grid a:hover figcaption p {
  opacity: 0;
}

/* Individual styles */
.demo-1 body {
  background: #3498db;
}

.grid figure span{
  font-family:$regular; 
  top: 70%;
  left: 50%;
  border: 3px solid #fff;
  background: transparent;
  color: #fff;
  opacity: 0;
  -webkit-transform: translateY(-50%) translateX(-50%) scale(0.25);
  transform: translateY(-50%) translateX(-50%) scale(0.25);
}

.grid a:hover figure span{
  opacity: 1;
  -webkit-transform: translateY(-50%) translateX(-50%) scale(1);
  transform: translateY(-50%) translateX(-50%) scale(1);
}


/* Media Queries */
/* Let's redefine the width of each anchor and the margins */

@media screen and (max-width: 58em) {
  .grid a {
    width: 33.333%;
  }

  .grid a:nth-child(odd) {
    margin: 0;
  }

  .grid a:nth-child(3n-1) {
    margin: 30px 0 -30px 0;
  }
}

@media screen and (max-width: 45em) {
  .grid {
    max-width: 500px;
  }

  .grid a {
    width: 50%;
  }

  .grid a:nth-child(3n-1) {
    margin: 0;
  }

  .grid a:nth-child(even) {
    margin: 30px 0 -30px 0;
  }


  .grid figcaption h2 {
    margin-bottom: 0px;
    -webkit-transform: translateY(30px);
    transform: translateY(30px);
  }

  .grid figcaption p {
    margin: 0;
    padding: 0 10px;
  }
}

@media screen and (max-width: 27em) {
  .grid {
    max-width: 250px;
  }

  .grid a {
    width: 100%;
  }

  .grid a:nth-child(even) {
    margin: 0;
  }
}

/*=============== Contacto =================*/

#video_background {
  position: fixed; 
  left: 0px; 
  top: 0px; 
  width: 100%; 
  height: 100%; 
  width: auto; 
  height: auto; 
  z-index: -1000; 
  overflow: hidden; 
  -webkit-filter: blur(3px);


}
#video_pattern { 
  background:url(../img/layout/somosBg.png);
  position: fixed;  
  left: 0px; 
  top: 0px; 
  width: 100%; 
  height: 100%; 
  z-index: -500; 
  opacity:0.9;
}


#contacto{
  position: relative;
  height:1000px;
  .infoContacto{
    width:1200px;
    margin:8em auto;
    z-index:200;
    form, > div{
      float:left;
      width:50%;
    }
     form{
           -webkit-box-sizing: border-box; /* Safari/Chrome, other WebKit */
   -moz-box-sizing: border-box;    /* Firefox, other Gecko */
   box-sizing: border-box;      
      padding-left:5%;
      label{
        display:block;
        color:white;
        font-family:$light;
        font-size:2em;
        margin-bottom:1em;
      }
      input{
        display:block;
        margin-top:0.5em; 
        height:2em;
        line-height:2em;
        width:60%;
        font-size:0.5em;
        padding-left:2%; 
         @include border-radius(4px);
      }
      textarea{
        font-size:0.5em;
        width:60%;
        padding-left:2%;
      }
      input[type="submit"]{
        font-size:1.2em;
        font-family:$regular;
        color:white;
        border:0;
        border-top:2px solid white;
        padding-top:0.5em;
        background:none;
        display:inline-block;
        letter-spacing:0.1em;
        &:hover{
          color:$aqua;
        }
      }
    }
    h3{
      font-size:3em;
      font-family:$regular;
      color:$aqua;
      letter-spacing:3px;
      margin-bottom:0.5em;
    }
    p{
      font-size: 2em;
      font-family:$thin;
      color:white;
      letter-spacing:3px;
      margin-bottom:0.5em;
      &:last-child{
        color:#82fef7;
      }
    }
    ul li{
      margin-top:2em;
      display:inline-block;
      width:8.5%;
      height:2.4em;
      a{
        height:2.4em;
        width:100%;
        &.fb{background-position: -85px 0;}
        &.tw{background-position: 0 0;}
        &.in{background-position: -156px 0;}
        &.sk{background-position: -235px 0;}
        &:hover{
          &.fb{background-position: -85px -67px;}
          &.tw{background-position: 0 -67px;}
          &.in{background-position: -156px -67px;}
          &.sk{background-position: -235px -67px;}
        }
      }
    }
  }
  .footer{
    position:relative;
    bottom:0;
    text-align:center;
    p{
      border-top:1px solid white; 
      padding-top:1em;
      width:50%;
      margin:0 auto;
      display:inline-block;
      color:white;
      font-size: 1em;
      font-family:$regular;
      letter-spacing:0.4em;
      @include transition(all,0.5s,ease-in);
      &:hover{
        color:$aqua;
      }
    }
  }
}


/* ===================================== Helper Clases ===================================== */

.sprite{
    background-image:url(../img/layout/sprites.png);
    background-repeat:no-repeat;
    display:block;
}



img{
	max-width:100%;
}

.ir {
    background-color: transparent;
    border: 0;
    overflow: hidden;
    *text-indent: -9999px;
}

.ir:before {
    content: "";
    display: block;
    width: 0;
    height: 150%;
}

.hidden {
    display: none !important;
    visibility: hidden;
}

.visuallyhidden {
    border: 0;
    clip: rect(0 0 0 0);
    height: 1px;
    margin: -1px;
    overflow: hidden;
    padding: 0;
    position: absolute;
    width: 1px;
}

.visuallyhidden.focusable:active,
.visuallyhidden.focusable:focus {
    clip: auto;
    height: auto;
    margin: 0;
    overflow: visible;
    position: static;
    width: auto;
}

.invisible {
    visibility: hidden;
}

.clearfix:before,
.clearfix:after {
    content: " ";
    display: table;
}

.clearfix:after {
    clear: both;
}

.clearfix {
    *zoom: 1;
}
