  .infoContacto{
    width:1200px;
    margin:8em auto;
    z-index:200;
    form, > div{
      float:left;
      width:50%;
    }
     form{
           -webkit-box-sizing: border-box; /* Safari/Chrome, other WebKit */
   -moz-box-sizing: border-box;    /* Firefox, other Gecko */
   box-sizing: border-box;      
      padding-left:5%;
      label{
        display:block;
        color:white;
        font-family:$light;
        font-size:2em;
        margin-bottom:1em;
      }
      input{
        display:block;
        margin-top:0.5em; 
        height:2em;
        line-height:2em;
        width:60%;
        font-size:0.5em;
        padding-left:2%; 
         @include border-radius(4px);
      }
      textarea{
        font-size:0.5em;
        width:60%;
        padding-left:2%;
      }
      input[type="submit"]{
        font-size:1.2em;
        font-family:$regular;
        color:white;
        border:0;
        border-top:2px solid white;
        padding-top:0.5em;
        background:none;
        display:inline-block;
        letter-spacing:0.1em;
        &:hover{
          color:$aqua;
        }
      }
    }
    h3{
      font-size:3em;
      font-family:$regular;
      color:$aqua;
      letter-spacing:3px;
      margin-bottom:0.5em;
    }
    p{
      font-size: 2em;
      font-family:$thin;
      color:white;
      letter-spacing:3px;
      margin-bottom:0.5em;
      &:last-child{
        color:#82fef7;
      }
    }
    ul li{
      margin-top:2em;
      display:inline-block;
      width:8.5%;
      height:2.4em;
      a{
        height:2.4em;
        width:100%;
        &.fb{background-position: -85px 0;}
        &.tw{background-position: 0 0;}
        &.in{background-position: -156px 0;}
        &.sk{background-position: -235px 0;}
        &:hover{
          &.fb{background-position: -85px -67px;}
          &.tw{background-position: 0 -67px;}
          &.in{background-position: -156px -67px;}
          &.sk{background-position: -235px -67px;}
        }
      }
    }
  }
  .footer{
    position:relative;
    bottom:0;
    text-align:center;
    p{
      border-top:1px solid white; 
      padding-top:1em;
      width:50%;
      margin:0 auto;
      display:inline-block;
      color:white;
      font-size: 1em;
      font-family:$regular;
      letter-spacing:0.4em;
      @include transition(all,0.5s,ease-in);
      &:hover{
        color:$aqua;
      }
    }
  }
}


/* ===================================== Helper Clases ===================================== */

.sprite{
    background-image:url(../img/layout/sprites.png);
    background-repeat:no-repeat;
    display:block;
}



img{
	max-width:100%;
}

.ir {
    background-color: transparent;
    border: 0;
    overflow: hidden;
    *text-indent: -9999px;
}

.ir:before {
    content: "";
    display: block;
    width: 0;
    height: 150%;
}

.hidden {
    display: none !important;
    visibility: hidden;
}

.visuallyhidden {
    border: 0;
    clip: rect(0 0 0 0);
    height: 1px;
    margin: -1px;
    overflow: hidden;
    padding: 0;
    position: absolute;
    width: 1px;
}

.visuallyhidden.focusable:active,
.visuallyhidden.focusable:focus {
    clip: auto;
    height: auto;
    margin: 0;
    overflow: visible;
    position: static;
    width: auto;
}

.invisible {
    visibility: hidden;
}

.clearfix:before,
.clearfix:after {
    content: " ";
    display: table;
}

.clearfix:after {
    clear: both;
}

.clearfix {
    *zoom: 1;
}
