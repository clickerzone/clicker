/* VARIABLES Y MIXINS*/
$mxw:1200px;
$titleFont:'Arvo', serif;
$regular:'bebas_neueregular';
$bold:'bebas_neuebold';
$book:'bebas_neuebook';
$light:'bebas_neuelight';
$thin:'bebas_neuethin';
$aqua:#2DEAE0;


@mixin transition($prop, $duration, $timing){
  -webkit-transition: $prop $duration $timing;
  -moz-transition: $prop $duration $timing;
  -o-transition: $prop $duration $timing;
  transition: $prop $duration $timing;
}
@mixin transform($rotate,$scale,$skew,$translate){
  transform: rotate($rotate) scale($scale) skew($skew) translate($translate);
  -webkit-transform: rotate($rotate) scale($scale) skew($skew) translate($translate);
  -moz-transform: rotate($rotate) scale($scale) skew($skew) translate($translate);
  -o-transform: rotate($rotate) scale($scale) skew($skew) translate($translate);
  -ms-transform: rotate($rotate) scale($scale) skew($skew) translate($translate);
}


@mixin border-radius($radius) {
  -webkit-border-radius: $radius;
     -moz-border-radius: $radius;
      -ms-border-radius: $radius;
       -o-border-radius: $radius;
          border-radius: $radius;
}

/* ===================================== Site ===================================== */

.wrapper{margin:0 auto;max-width:$mxw;}
.cursor{cursor: url(../img/layout/clickerCursor.svg), auto;height:100%;width:100%;position:absolute;top:0;left:0;}
a{cursor: url(../img/layout/pointer.svg), auto;}

/* ===================================== Header ===================================== */

header{
  position:fixed;
  z-index:6;
  width:100%;
  .invisible{display:none}
  h1{
    @include transition(all,0.5s,ease-in);
    margin:1em 0 0.7em;
    float:left;
    width:120px/$mxw*100%;
    height:2.5em;
    a{
      display:block;
      width:100%;
      height:2.5em;
      background:url(../img/content/logo.svg)no-repeat;
    }
  }
  nav{
    width:432px/$mxw*100%;
    float:right;
    a{
      font-family:$titleFont;
      font-size:0.9em;
      float:left;
      padding:2em 12px/462px*100% 1em;
      display:block;
      text-decoration:none;
      text-transform:uppercase;
      color:white;
      @include transition(all,0.5s,ease-in);
      &:hover{
        color:$aqua;
      }
      &:first-child{
        margin-left:0;
      }
    }
  }
}

/* ===================================== Clase para todas las secciones ===================================== */
.story { height: 1000px; padding: 0; margin: 0; width: 100%; max-width: 1920px; position: relative; margin: 0 auto; border-top: 1px solid rgba(255,255,255,0.3); border-bottom: 1px solid rgba(0,0,0,0.4); box-shadow: 0 0 50px rgba(0,0,0,0.8);} 



/* ===================================== Seccion Somos ===================================== */
